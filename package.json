{
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "format": "prettier \"**/*.{md,js,jsx,json,css}\" --write --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:links && npm run lint:spelling",
    "lint:js": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:md": "write-good $(glob 'content/**/*.md') --no-tooWordy",
    "lint:links": "remark -f content/",
    "lint:spelling": "cspell '**/*.{md,js,jsx,json,css,yml}'",
    "deploy": "netlify deploy --prod --dir=public --message=\"$TRAVIS_COMMIT_MESSAGE\"",
    "test": "npm run lint"
  },
  "remarkConfig": {
    "plugins": [
      "remark-lint-no-dead-urls"
    ]
  },
  "private": true,
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.5.5",
    "gatsby-image": "^2.1.0",
    "gatsby-plugin-canonical-urls": "^2.1.3",
    "gatsby-plugin-catch-links": "^2.0.15",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.37",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.0.14",
    "gatsby-remark-katex": "^3.0.4",
    "gatsby-remark-prismjs": "^3.2.11",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.1.19",
    "gatsby-transformer-yaml": "^2.2.4",
    "katex": "^0.10.2",
    "polished": "^3.4.0",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0",
    "react-spring": "^8.0.20",
    "styled-components": "^4.2.0",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cli-glob": "^0.1.0",
    "cspell": "^4.0.23",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "netlify-cli": "^2.12.0",
    "prettier": "^1.17.1",
    "puppeteer": "^1.19.0",
    "remark-cli": "^6.0.1",
    "remark-lint-no-dead-urls": "^0.4.1",
    "write-good": "^1.0.1"
  },
  "license": "MIT"
}
