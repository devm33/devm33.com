{
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "format": "prettier \"**/*.{md,js,jsx,json,css}\" --write --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:links && npm run lint:spelling",
    "lint:js": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:md": "write-good $(glob 'content/**/*.md') --no-tooWordy",
    "lint:links": "remark -f content/",
    "lint:spelling": "cspell '**/*.{md,js,jsx,json,css,yml}'",
    "deploy": "netlify deploy --prod --dir=public --message=\"$TRAVIS_COMMIT_MESSAGE\"",
    "test": "npm run lint"
  },
  "remarkConfig": {
    "plugins": [
      "remark-lint-no-dead-urls"
    ]
  },
  "private": true,
  "dependencies": {
    "gatsby": "^5.6.1",
    "gatsby-plugin-canonical-urls": "^5.6.0",
    "gatsby-plugin-catch-links": "^5.6.0",
    "gatsby-plugin-image": "^3.6.0",
    "gatsby-plugin-manifest": "^5.6.0",
    "gatsby-plugin-minify-classnames": "^5.0.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-sharp": "^5.6.0",
    "gatsby-plugin-sitemap": "^6.6.0",
    "gatsby-remark-copy-linked-files": "^6.6.0",
    "gatsby-remark-images": "^7.6.0",
    "gatsby-remark-katex": "^7.6.0",
    "gatsby-remark-prismjs": "^7.6.0",
    "gatsby-source-filesystem": "^5.6.0",
    "gatsby-transformer-remark": "^6.6.0",
    "gatsby-transformer-sharp": "^5.6.0",
    "gatsby-transformer-yaml": "^5.6.0",
    "katex": "^0.13.3",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-spring": "^9.6.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "cspell": "^6.26.3",
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "netlify-cli": "^12.12.1",
    "prettier": "^2.8.4",
    "puppeteer": "^19.7.1",
    "remark-cli": "^11.0.0",
    "remark-lint-no-dead-urls": "^1.1.0",
    "write-good": "^1.0.8"
  },
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "license": "MIT"
}
